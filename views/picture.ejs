<!doctype html>
<html lang="ru-RU">
<head>

    <% include includes/head.ejs %>

</head>
<body>
<div class="wrapper">

    <% include includes/header.ejs %>

    <div class="container">
        <main>
            <div class="main">

                <div class="sbar">
                    <div class="speedbar">
						<span>
							<a href="<%- page.protocol %><%- page.domain %>">
								<span>Главная</span>
							</a>
						</span> »
                        <span>
							<%- movie.genre_url %>
						</span> »
                        <span>
							<a href="<%- movie.url %>">
								<span><%- movie.title_ru %></span>
							</a>
						</span> »
                        Кадры
                    </div>
                </div>

                <div id="cinemapress-content">
                    <div class="short_zombiz">
                        <div class="title-full">
                            <img src="/themes/ramsay/public/images/video-play.png" class="h2-title-short" alt="Онлайн">
                            <h1>Кадры из <%- movie.title_ru %> <%- movie.year %></h1>
                        </div>

                        <% if (page.seo) { %>

                        <div class="fullstory">
                            <%- page.seo %>
                        </div>

                        <% } %>

                        <div class="fullstory episodesListBlock" style="display: none;">
                            <h3><%- movie.title %> новые серии</h3>
                            <div id="episodesList" data-id="<%- movie.kp_id %>"></div>
                        </div>

                        <div class="fullstory">
                            <div class="menuSch" style="left: inherit; top: inherit;">
                                <a href="<%- movie.url %>/<%- page.urls.movies.online %>">Онлайн</a>
                                <a href="<%- movie.url %>/<%- page.urls.movies.download %>">Скачать</a>
                                <a href="<%- movie.url %>/<%- page.urls.movies.trailer %>">Трейлер</a>
                                <a href="<%- movie.url %>/<%- page.urls.movies.picture %>" class="red"><strong>Кадры</strong></a>
                            </div>
                            <div class="nomt">
                                <div style="margin: 30px auto;">
                                    <div style="width: 100%; height: 1100px; position: relative;">
                                        <%- page.player %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div style="clear:both;"></div>
                    </div>
                </div>

            </div>
        </main>
        <aside>

            <% include includes/right.ejs %>

        </aside>
    </div>

    <% include includes/footer.ejs %>

</div>
</body>
</html>